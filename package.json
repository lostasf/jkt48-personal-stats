{
  "name": "ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python -m uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:studio": "npx drizzle-kit studio",
    "db:push": "npx drizzle-kit push",
    "db:prod": "tsx ./scripts/seed.ts",
    "db:drop": "npx drizzle-kit drop",
    "db:seed": "tsx ./scripts/seed.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@vercel/analytics": "^1.5.0",
    "cheerio": "^1.0.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5",
    "uuid": "^11.1.0"
  }
}
